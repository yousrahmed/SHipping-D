@model IEnumerable<Shipping.Models.Craditcarrd>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Mymester.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.C_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.C_address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.C_Number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vcs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Expirydate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.C_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.C_address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.C_Number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vcs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Expirydate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
